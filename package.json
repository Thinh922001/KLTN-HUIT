{
  "name": "KLTN",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json && yarn copy:templates",
    "start:dev": "tsnd src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start": "node dist/src/main.js",
    "test": "jest --detectOpenHandles --runInBand --forceExit",
    "test:clean-cache": "jest --clear-cache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn test:clean-cache && DOTENV_CONFIG_PATH=env/.env.test node --expose-gc ./node_modules/.bin/jest --config ./__tests__/jest-e2e.json --verbose --runInBand --logHeapUsage --forceExit",
    "test:e2e:cov": "DOTENV_CONFIG_PATH=env/.env.test jest --config ./jest-cov-e2e.json --verbose --runInBand --coverage",
    "test:e2e:file": "yarn run test:e2e -t",
    "start-docker": "export NODE_ENV=docker && docker-compose -f ./docker-compose.yml up --build -d",
    "start-local-docker": "export NODE_ENV=docker && docker-compose -f ./docker-compose.dev.yml up --build",
    "stop-local-docker": "export NODE_ENV=docker && docker-compose -f ./docker-compose.dev.yml down",
    "prepare": "husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migrations:run": "yarn run typeorm migration:run -- -d ./typeOrm.config.ts",
    "migration:generate": "yarn run typeorm -- -d ./typeOrm.config.ts migration:generate ./src/migrations/add_column_refresh_token_table_admin",
    "migration:create": "yarn run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "migration:revert": "yarn run typeorm -- -d ./typeOrm.config.ts migration:revert",
    "copy:templates": "cpx './src/services/mail/templates/**' 'dist/src/services/mail/templates' --clean"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.699.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/platform-socket.io": "^10.4.6",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^10.4.6",
    "@turf/distance": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@types/lodash": "^4.14.188",
    "@types/multer": "^1.4.12",
    "axios": "^1.7.7",
    "base64-img": "^1.0.4",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.4",
    "cpx": "^1.5.0",
    "crypto-js": "^4.1.1",
    "custom-uuid": "^3.0.0",
    "dotenv": "^16.0.3",
    "expect-more-jest": "^5.4.1",
    "express-rate-limit": "^6.6.0",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "jwks-rsa": "^3.0.1",
    "local": "^0.3.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mysql-error-codes": "^0.1.3",
    "mysql2": "^2.3.3",
    "nest-schedule": "^0.6.4",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.10",
    "typeorm-transactional": "^0.4.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.6",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/hapi__joi": "^17.1.8",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "faker": "^6.6.6",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "mocker-data-generator": "^2.12.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "superagent": "^8.0.3",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4",
    "xmlhttprequest": "^1.8.0",
    "yarn-upgrade-all": "^0.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 50000
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pretty-quick --staged"
    }
  }
}
